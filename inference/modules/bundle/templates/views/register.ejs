<%- include('header', locals) %>
<%- include('nav', locals) %>
<div class="container" style="
  display: flex;
  align-items: center;
  flex-direction: column;
  margin: auto;
">

<form style="width: 300px;" class="needs-validation" novalidate onsubmit="handleSubmission(event)" method="POST">
<div class="form-group">
  <h2>Create an account.</h2>
</div>
<div class="form-group">
  <label for="inputUsername">Username</label>
  <div class="input-group">
  <div class="input-group-prepend">
      <div class="input-group-text">
        <div style="
          background-image: url('data:image/svg+xml;base64,');
          height: 24px;
          width: 24px;
          background-position: center center;
          background-repeat: no-repeat;
          background-size: contain;
          opacity: 0.6;"
        "></div>
      </div>
    </div>
  <input required name="username" type="username" id="inputUsername" class='form-control'>
</div>
</div>
<div class="form-group">
<label for="inputPassword6">Password</label>
<div class="input-group">
  <div class="input-group-prepend">
      <div class="input-group-text">
        <div style="
          background-image: url('data:image/svg+xml;base64,');
          height: 24px;
          width: 24px;
          background-position: center center;
          background-repeat: no-repeat;
          background-size: contain;
          opacity: 0.6;"
        "></div>
      </div>
    </div>
  <input  required name="password" type="password" id="inputPassword6" class="form-control" aria-describedby="passwordHelpInline">
</div>


</div>
<div class="form-group">
<label for="inputPassword6">Confirm Password</label>
<div class="input-group">
  <div class="input-group-prepend">
      <div class="input-group-text">
        <div style="
          background-image: url('data:image/svg+xml;base64,');
          height: 24px;
          width: 24px;
          background-position: center center;
          background-repeat: no-repeat;
          background-size: contain;
          opacity: 0.6;"
        "></div>
      </div>
    </div>
  <input  required name="confirmPassword" type="password" id="inputConfirmPassword7" class="form-control" aria-describedby="passwordHelpInline">
  <div class="invalid-feedback">
    Passwords must match and be between 8-20 characters.
  </div>
</div>

</div>

<div class="form-group form-row">
  <button class="mr-1 col btn btn-primary" type="submit">Register</button>
  <a href="/login" class="ml-1 col btn btn-secondary">Log in</a>
</div>
</div>
</form>
<!-- Handle Form submission and client-side verification -->
<script>

function handleSubmission(e){
  var form = e.srcElement;
  var formData = new FormData(form);
  if (!form.checkValidity() || (formData.get('password') !== formData.get('confirmPassword'))){
    e.preventDefault();
    form.classList.add('was-validated');
  }

}

</script>
